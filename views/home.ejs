                            <a href="#"><%= womenProducts[i].title %></a>
                          </h4>
                          <span class="aa-product-price">&#8377 <%= womenProducts[i].price %></span
                          >
                          <!-- <span class="aa-product-price"
                            ><del>&#8377 650.50</del></span
                          > -->
                        </figcaption>
                      </figure>
                      <div class="aa-product-hvr-content">
                        <a
                          href="#"
