<!-- Start slider -->
<section id="aa-slider">
  <div class="aa-slider-area">
    <div id="sequence" class="seq">
      <div class="seq-screen">
        <ul class="seq-canvas">
          <!-- single slide item -->
          <li>
            <div class="seq-model">
              <img data-seq src="img/slider/7.jpg" alt="Men slide img" />
            </div>
            <div class="seq-title">
              <span data-seq>Save Up to 75% Off</span>
              <h2 data-seq>Men Collection</h2>
              <p data-seq>
                Have a stylish look
              </p>
              <a data-seq href="#" class="aa-shop-now-btn aa-secondary-btn"
                >SHOP NOW</a
              >
            </div>
          </li>
          <!-- single slide item -->
          <li>
            <div class="seq-model">
              <img data-seq src="img/slider/9.jpg" alt="Wristwatch slide img" />
            </div>
            <div class="seq-title">
              <span data-seq>Save Up to 40% Off</span>
              <h2 data-seq>Women Collection</h2>
              <p data-seq>
                Attractive designs in modern styles.
              </p>
              <a data-seq href="#" class="aa-shop-now-btn aa-secondary-btn"
                >SHOP NOW</a
              >
            </div>
          </li>
          <!-- single slide item -->
          <li>
            <div class="seq-model">
              <img
                data-seq
                src="img/slider/8.jpg"
                alt="Women Jeans slide img"
              />
            </div>
            <div class="seq-title">
              <span data-seq>Save Up to 50% Off</span>
              <h2 data-seq>Furniture Collection</h2>
              <p data-seq>
                High Quality Furnitures with variety of brands.
              </p>
              <a data-seq href="#" class="aa-shop-now-btn aa-secondary-btn"
                >SHOP NOW</a
              >
            </div>
          </li>
        </ul>
      </div>
      <!-- slider navigation btn -->
      <fieldset
        class="seq-nav"
        aria-controls="sequence"
        aria-label="Slider buttons"
      >
        <a type="button" class="seq-prev" aria-label="Previous"
          ><span class="fa fa-angle-left"></span
        ></a>
        <a type="button" class="seq-next" aria-label="Next"
          ><span class="fa fa-angle-right"></span
        ></a>
      </fieldset>
    </div>
  </div>
</section>
<!-- / slider -->
<!-- Products section -->
<section id="aa-product">
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div class="row">
          <div class="aa-product-area">
            <div class="aa-product-inner">
              <!-- start prduct navigation -->
              <ul class="nav nav-tabs aa-products-tab">
                <li class="active"><a href="#men" data-toggle="tab">Men</a></li>
                <li><a href="#women" data-toggle="tab">Women</a></li>
                <li><a href="#furniture" data-toggle="tab">Furniture</a></li>
                <!-- <li>
                  <a href="#electronics" data-toggle="tab">Electronics</a>
                </li> -->
              </ul>
              <!-- Tab panes -->
              <div class="tab-content">
                
                <!-- Start men product category -->
                <div class="tab-pane fade in active" id="men">
                  <ul class="aa-product-catg">
                    <% for (i=0; i < menProducts.length; i++) { %>
                    <!-- start single product item -->
                    <li>
                      <figure>
                        <a class="aa-product-img" href="/cart/add/<%= menProducts[i].id %>">
                          <img
                            style="width: 100%"
                            src="<%= menProducts[i].img_url %>"
                            alt="Men Dress"
                          />
                      </a>
                        <a class="aa-add-card-btn" href="/cart/add/<%= menProducts[i].id %>">
                          <span class="fa fa-shopping-cart"></span>
                          Add To Cart
                        </a>
                        <figcaption>
                          <h4 class="aa-product-title">
                            <a href="#"><%= menProducts[i].title %></a>
                          </h4>
                          <span class="aa-product-price">&#8377 <%= menProducts[i].price %></span>
                          <!-- <span class="aa-product-price"><del>&#8377 650.50</del></span> -->
                        </figcaption>
                      </figure>
                      <div class="aa-product-hvr-content">
                        <a
                          href="#"
                          data-toggle="tooltip"
                          data-placement="top"
                          title="Add to Wishlist"
                          ><span class="fa fa-heart-o"></span
                        ></a>
                        <a
                          href="#"
                          data-toggle="tooltip"
                          data-placement="top"
                          title="Compare"
                          ><span class="fa fa-exchange"></span
                        ></a>
                        <a
                          href="#"
                          data-toggle2="tooltip"
                          data-placement="top"
                          title="Quick View"
                          data-toggle="modal"
                          data-target="#quick-view-modal"
                          ><span class="fa fa-search"></span
                        ></a>
                      </div>
                      
                    </li>
                    <% } %>

                  </ul>
                  <!-- <a class="aa-browse-btn" href="#"
                    >Browse all Product
                    <span class="fa fa-long-arrow-right"></span
                  ></a> -->
                </div>
                <!-- / men product category -->

                <!-- start women product category -->
                <div class="tab-pane fade" id="women">
                  <ul class="aa-product-catg">
                    <% for (i=0; i < womenProducts.length; i++) { %>
                    <!-- start single product item -->
